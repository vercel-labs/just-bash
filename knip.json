{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/cli/just-bash.ts",
    "src/index.ts",
    "src/workflow.ts",
    "src/browser.ts"
  ],
  "project": ["src/**/*.ts"],
  "ignore": ["src/**/*.test.ts", "src/spec-tests/**"],
  "ignoreBinaries": ["tsx", "dev"],
  "rules": {
    "unlisted": "off",
    "unresolved": "off",
    "optionalPeerDependencies": "off",
    "types": "off"
  }
}
