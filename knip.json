{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/cli/bash-env.ts"],
  "project": ["src/**/*.ts"],
  "ignore": ["src/**/*.test.ts", "src/spec-tests/**"],
  "ignoreBinaries": ["tsx"],
  "rules": {
    "unlisted": "off",
    "unresolved": "off",
    "optionalPeerDependencies": "off",
    "types": "off"
  }
}
